{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "description": "This schema represents the Dutch Eating Behavior Questionnaire.",
    "type": "object",
    "references": [
        {
            "description": "Van Strien et al. (1986)",
            "url": "http://onlinelibrary.wiley.com/doi/10.1002/1098-108X(198602)5:2%3C295::AID-EAT2260050209%3E3.0.CO;2-T/abstract"
        }
    ],
    "definitions": {
        "item": {
            "type": "object",
            "properties": {
                "question": {
                    "type": "object",
                    "properties": {
                        "code": {
                            "type": "string"
                        },
                        "text": {
                            "type": "string"
                        }
                    },
                    "required": [
                        "text"
                    ]
                },
                "answer": {
                    "type": "object",
                    "enum": [
                        {
                            "code": 1,
                            "text": "never"
                        },
                        {
                            "code": 2,
                            "text": "seldom"
                        },
                        {
                            "code": 3,
                            "text": "sometimes"
                        },
                        {
                            "code": 4,
                            "text": "often"
                        },
                        {
                            "code": 5,
                            "text": "very often"
                        },
                        {
                            "code": "NR",
                            "text": "not relevant"
                        }

                    ],
                    "additionalItems": false
                }
            },
            "additionalProperties": false
        },
        "time_frame": {
            "$ref": "time-frame-1.x.json"
        }
    },
    "properties": {
        "items": {
            "type": "array",
            "items": [
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee11",
                                            "text": "Do you have the desire to eat when you are irritated?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee12",
                                            "text": "Do you have a desire to eat when you have nothing to do?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee13",
                                            "text": "Do you have a desire to eat when you are depressed or discouraged?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee14",
                                            "text": "Do you have a desire to eat when you are feeling lonely?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee15",
                                            "text": "Do you have a desire to eat when somebody lets you down?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee16",
                                            "text": "Do you have a desire to eat when you are cross?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee17",
                                            "text": "Do you have a desire to eat when you are approaching something unpleasant to happen?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee18",
                                            "text": "Do you have a desire to eat when you are anxious, worried or tense?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee19",
                                            "text": "Do you have a desire to eat when things are going against you or when things have gone wrong?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee20",
                                            "text": "Do you have a desire to eat when you are frightened?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee21",
                                            "text": "Do you have a desire to eat when you are disappointed?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee22",
                                            "text": "Do you have a desire to eat when you are emotionally upset?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                },
                {
                    "type": "object",
                    "allOf": [
                        {
                            "$ref": "#/definitions/item"
                        },
                        {
                            "properties": {
                                "question": {
                                    "enum": [
                                        {
                                            "code": "Debq_ee23",
                                            "text": "Do you have a desire to eat when you are bored or restless?"
                                        }
                                    ]
                                }
                            }
                        }
                    ]
                }
            ],
            "additionalItems": false
        },
        "emotional_eating_scale": {
            "type": "number",
            "description": "DEBQ_EE = (DEBQ_EE11 + DEBQ_EE12 + DEBQ_EE13 + DEBQ_EE14 + DEBQ_EE15 + DEBQ_EE16 + DEBQ_EE17 + DEBQ_EE18 + DEBQ_EE19 + DEBQ_EE20 + DEBQ_EE21 + DEBQ_EE22 + DEBQ_EE23)/13"
        },
        "effective_time_frame": {
            "allOf": [
                {
                    "$ref": "#/definitions/time_frame"
                },
                {
                    "required": [
                        "date_time"
                    ]
                }
            ]
        }
    },
    "additionalProperties": false
}